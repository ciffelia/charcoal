{
  "name": "@charcoal-ui/react-sandbox",
  "version": "2.0.0-alpha.8",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    "import": "./dist/index.mjs",
    "default": "./dist/index.cjs"
  },
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "analyze": "npm-run-all --parallel --aggregate-output \"analyze:*\"",
    "analyze:cjs": "zx ../../misc/bundler.mjs --format=cjs --analyze",
    "analyze:esm": "zx ../../misc/bundler.mjs --format=esm --analyze",
    "build": "npm-run-all --parallel --aggregate-output \"build:*\"",
    "build:cjs": "zx ../../misc/bundler.mjs --format=cjs",
    "build:esm": "zx ../../misc/bundler.mjs --format=esm",
    "build:declaration": "tsc --project tsconfig.build.json --declaration --emitDeclarationOnly",
    "typecheck": "tsc --project tsconfig.build.json --noEmit",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.17",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addons": "^6.4.17",
    "@storybook/api": "^6.4.17",
    "@storybook/components": "^6.4.17",
    "@storybook/core-events": "^6.4.17",
    "@storybook/react": "^6.4.17",
    "@storybook/theming": "^6.4.17",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/jest-axe": "^3.5.3",
    "@types/react": "^17.0.38",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.21",
    "@types/warning": "^3.0.0",
    "esbuild": "^0.15.7",
    "jest-axe": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-spring": "^9.4.2",
    "rimraf": "^3.0.2",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.5",
    "zx": "^7.0.3"
  },
  "dependencies": {
    "@charcoal-ui/foundation": "^1.0.1-alpha.0",
    "@charcoal-ui/react": "^2.0.0-alpha.19",
    "@charcoal-ui/styled": "^2.0.0-alpha.2",
    "@charcoal-ui/theme": "^2.0.0-alpha.4",
    "@charcoal-ui/utils": "^1.1.0-alpha.0",
    "polished": "^4.1.4",
    "warning": "^4.0.3"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "react-spring": "^9.0.0-beta.34",
    "styled-components": ">=5.1.1"
  },
  "files": [
    "src",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pixiv/charcoal.git",
    "directory": "packages/react-sandbox"
  }
}
