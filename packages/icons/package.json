{
  "name": "@charcoal-ui/icons",
  "version": "1.1.0-alpha.5",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    "import": "./dist/index.mjs",
    "default": "./dist/index.cjs"
  },
  "types": "./dist/index.d.ts",
  "sideEffects": true,
  "scripts": {
    "analyze": "npm-run-all --parallel --aggregate-output \"analyze:*\"",
    "analyze:cjs": "zx ../../misc/bundler.mjs --format=cjs --analyze",
    "analyze:esm": "zx ../../misc/bundler.mjs --format=esm --analyze",
    "build": "npm-run-all --parallel --aggregate-output \"build:*\"",
    "build:cjs": "zx ../../misc/bundler.mjs --format=cjs",
    "build:esm": "zx ../../misc/bundler.mjs --format=esm",
    "build:declaration": "tsc --project tsconfig.build.json --declaration --emitDeclarationOnly",
    "typecheck": "tsc --project tsconfig.build.json --noEmit",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/dompurify": "^2.3.3",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/warning": "^3.0.0",
    "esbuild": "^0.15.7",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.5",
    "zx": "^7.0.3"
  },
  "dependencies": {
    "dompurify": "^2.3.6",
    "warning": "^4.0.3"
  },
  "files": [
    "src",
    "dist",
    "svg"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pixiv/charcoal.git",
    "directory": "packages/icons"
  }
}
